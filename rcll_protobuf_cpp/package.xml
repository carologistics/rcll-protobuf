<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>rcll_protobuf_cpp</name>
  <version>0.0.0</version>
  <description>C++ bindings for RCLL protobuf messages</description>
  <maintainer email="viehmann@kbsg.rwth-aachen.de">Tarik Viehmann</maintainer>
  <license>GPLv2+ license</license>

  <build_depend>rcll_protobuf_msgs</build_depend>
  <build_depend>protobuf</build_depend>
  <build_depend>protobuf-dev</build_depend>
  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
